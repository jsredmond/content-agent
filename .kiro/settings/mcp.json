        "get_provider_details"
      ]
    },
    "Snyk": {
      "command": "npx",
      "args": [
        "-y",
        "snyk@1.1300.0",
        "mcp",
        "-t",
        "stdio"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": []
    }
  }
}