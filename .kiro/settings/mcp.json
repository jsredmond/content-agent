      "args": [
        "awslabs.aws-documentation-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "search_documentation",
        "read_documentation"
      ]
    },
    "terraform": {
      "command": "/Users/jeremyredmond/go/bin/terraform-mcp-server",
      "args": [],
      "disabled": false,
